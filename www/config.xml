<?xml version='1.0' encoding='utf-8'?>
<widget id="com.webcomum.myfirstapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MyFirstApp</name>
    <description>
        A sample Webcomum application that responds to the deviceready event.
    </description>
    <author email="suporte@webcomum.com" href="http://www.webcomum.com">
        Webcomum Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <feature name="Accelerometer">
        <param name="android-package" value="org.apache.cordova.AccelListener" />
    </feature>
</widget>
